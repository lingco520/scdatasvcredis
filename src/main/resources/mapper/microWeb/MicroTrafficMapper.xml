<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.spring.springboot.dao.microWeb.MicroTrafficMapper">


    <select id="findAll" resultType="org.spring.springboot.domain.mysqlBean.microWeb.NavigationManage" parameterType="String">
        select s.imgUrl,s.introduction,s.vcode
        from w_navigation_manage s WHERE s.vcode=#{vcode} and s.id=158;
    </select>
</mapper>